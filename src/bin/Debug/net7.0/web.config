<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="redirect to BellaAPI">
                    <match url="(.*)" />
                    <action type="Rewrite" url="http://localhost:6001/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>